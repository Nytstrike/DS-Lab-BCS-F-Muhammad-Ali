#include <iostream>
using namespace std;

void printNumbers(int n) {

    if (n < 1) return;

    cout << n << " ";
    printNumbers(n - 1);
}
//24k1004
int main() {
    int n = 5;
    cout << "Direct Recursion: Numbers from " << n << " down to 1: ";
    printNumbers(n);
    cout << endl;
    return 0;
}
